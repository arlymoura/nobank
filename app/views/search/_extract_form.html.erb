<%= form_tag(account_search_index_path(current_user.account), method: :get) do %>
  <div class="form-group">
    <%= label_tag :first_date %>
    <%= text_field_tag :first_date %>
  </div>
  <div class="form-group">
    <%= label_tag :last_date %>
    <%= text_field_tag :last_date %>
  </div>
  <%= submit_tag "Search", class: 'btn btn-success' %>
<% end %>